package twovstwo;

public class Player {
	private String name;
	private int wintimes;

	public Player(String playername) {
		this.name = playername;
	}

	public String getName() {
		return this.name;
	}

	public int getWintimes() {
		return this.wintimes;
	}

	public void setWintimes() {
		this.wintimes++;
	}

}
